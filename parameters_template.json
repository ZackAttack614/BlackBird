    },
    "training": {
      "optimizer": "adam",
      "learning_rate": 0.001,
      "momentum": 0.9,
      "batch_size": 50
    }
  },
  "tensorflow": {
    "GPUOptions": {
      "per_process_gpu_memory_fraction": 0.2
    }
  }
}