{
  "mcts": {
    "maxDepth": 10,
    "explorationRate": 0.85,
    "timeLimit": null,
    "playLimit": 250,
    "temperature": {
      "exploration": 1,
      "exploitation": 0.1
    }
  },
  "network": {
